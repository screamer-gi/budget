{
    "name": "screamer/budget",
    "description": "budget",
    "keywords": [
        "budget"
    ],
    "require": {
        "php": "^7.2",
        "doctrine/doctrine-laminas-hydrator": "^2.0",
        "doctrine/doctrine-module": "^3.0",
        "doctrine/doctrine-orm-module": "dev-feature/laminas-convert2",
        "doctrine/orm": "^2.7",
        "laminas/laminas-di": "^3.1",
        "laminas/laminas-form": "^2.14",
        "laminas/laminas-i18n": "^2.10",
        "laminas/laminas-mvc": "^3.1",
        "laminas/laminas-navigation": "^2.9",
        "lstrojny/functional-php": "@stable"
    },
    "require-dev": {
        "laminas/laminas-component-installer": "^2.1"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/screamer-gi/DoctrineORMModule.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/",
            "Common\\": "module/Common/src/",
            "Expense\\": "module/Expense/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
